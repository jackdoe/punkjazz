{
  "expo": {
    "name": "punkjazz",
    "slug": "punkjazz",
    "version": "1.0.1",
    "icon": "./assets/icon.png",
    "description": "punkjazz is a local search engine for gutenberg.org's catalog",
    "primaryColor": "#000000",
    "githubUrl": "https://github.com/jackdoe/punkjazz",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.punkjazz",
      "buildNumber": 2
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "org.punkjazz",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
